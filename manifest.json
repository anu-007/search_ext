{
    "manifest_version": 3,
    "name": "Smart Search New Tab",
    "version": "1.1",
    "description": "Enhanced new tab page with smart search capabilities",
    "permissions": [
        "search"
    ],
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}