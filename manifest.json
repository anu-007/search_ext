{
    "manifest_version": 3,
    "name": "Smart Search Extension",
    "version": "1.0",
    "description": "Quick access to search engines and tech news",
    "permissions": [
        "tabs",
        "storage",
        "bookmarks"
    ],
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "32": "icons/icon32.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}